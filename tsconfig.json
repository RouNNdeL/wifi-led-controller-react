{
  "compilerOptions": {
    "sourceMap": true,
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es5",
    "lib": [
      "es6",
      "dom"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "jsx": "react",
    "forceConsistentCasingInFileNames": false
  }
}